<form class="form-group" id="get-directions-form">
  
  <div class="col-md-6 col-md-offset-3 input-group">
  <label class="sr-only">Start</label>
    <input class="form-control input" type="text" id="start" placeholder="Starting Point">
    <span class="input-group-addon"><img class="current-location" src="http://i.imgur.com/pGM46FY.png"></span>
  </div>
  
  <div class="col-sm-12 col-xs-12 col-md-6 col-md-offset-3 input-group">   
    <label class="sr-only"> End </label>
      <input class="form-control input" type="text" id="end" placeholder="Destination">
  </div>

  <div style="display:none" class="col-md-6 col-md-offset-3">
    <label class="sr-only input">Route Type:</label>
      <select class="form-control route-drop-down" id = "transit-type">
        <option value = "BICYCLING">Bike Paths</option>
        <option value = "DRIVING">More Direct</option>
      </select>
  </div>
  
  <div class="col-md-4 col-md-offset-4">
    <button class="btn btn-primary form-control" id="submit-button" type='submit'>Get Directions</button>
  </div>

</form>

<div class="row">
  <div style="height:50px"></div>
</div>
<div id="map-canvas"/></div>

<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-md-4" id="directionsPanel1">
      <h3 class="directions-discription" id="directions-info1"></h3>
      <h4 class="directions-description" id="station-status1"></h4>
    </div>
    <div class="col-xs-12 col-sm-6 col-md-4" id="directionsPanel2">
      <h3 class="directions-discription" id="directions-info2"></h3>
      <h4 class="directions-description" id="station-status2"><br></h4>
    </div>
    <div class="col-xs-12 col-sm-6 col-md-4" id="directionsPanel3">
      <h3 class="directions-discription" id="directions-info3"></h3>
      <h4 class="directions-description" id="station-status3"></h4>
    </div>
  </div>
</div>

